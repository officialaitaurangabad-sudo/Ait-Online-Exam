# Production Environment Configuration
VITE_NODE_ENV=production

# API Configuration
VITE_API_URL=https://api.your-domain.com/api
VITE_APP_NAME=AIT Online Exam Platform
VITE_APP_VERSION=1.0.0

# Features
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_DARK_MODE=true

# File Upload
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf

# Exam Settings
VITE_DEFAULT_EXAM_DURATION=60
VITE_AUTO_SAVE_INTERVAL=30000
VITE_WARNING_TIME=300

# UI Settings
VITE_ITEMS_PER_PAGE=10
VITE_DEBOUNCE_DELAY=300

# External Services
VITE_CLOUDINARY_CLOUD_NAME=your-production-cloud-name
VITE_CLOUDINARY_API_KEY=your-production-api-key

# Development
VITE_DEBUG_MODE=false
VITE_MOCK_API=false

# Analytics (Production)
VITE_GOOGLE_ANALYTICS_ID=GA-PROD-ID
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# WebSocket (Production)
VITE_WS_URL=wss://api.your-domain.com

# CDN
VITE_CDN_URL=https://cdn.your-domain.com

# Performance
VITE_ENABLE_SERVICE_WORKER=true
VITE_ENABLE_PWA=true
