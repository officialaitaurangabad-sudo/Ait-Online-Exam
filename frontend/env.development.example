# Development Environment Configuration
VITE_NODE_ENV=development

# API Configuration
VITE_API_URL=http://localhost:5000/api
VITE_APP_NAME=AIT Online Exam Platform (Dev)
VITE_APP_VERSION=1.0.0

# Features
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_DARK_MODE=true

# File Upload
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf

# Exam Settings
VITE_DEFAULT_EXAM_DURATION=60
VITE_AUTO_SAVE_INTERVAL=30000
VITE_WARNING_TIME=300

# UI Settings
VITE_ITEMS_PER_PAGE=10
VITE_DEBOUNCE_DELAY=300

# External Services
VITE_CLOUDINARY_CLOUD_NAME=dev-cloud-name
VITE_CLOUDINARY_API_KEY=dev-api-key

# Development
VITE_DEBUG_MODE=true
VITE_MOCK_API=false

# Analytics (Development)
VITE_GOOGLE_ANALYTICS_ID=GA-DEV-ID
VITE_SENTRY_DSN=https://dev-sentry-dsn@sentry.io/project-id

# WebSocket (Development)
VITE_WS_URL=ws://localhost:5000
